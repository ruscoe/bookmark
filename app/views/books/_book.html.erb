<section id="<%= dom_id book %>" class="book">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Owned on:</strong>
    <%= book.owned_on %>
  </p>

</section>
